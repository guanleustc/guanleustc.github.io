---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Le's Blog</title>
	<link href="http://blog.pragmaticlab.net/blog/atom.xml" rel="self"/>
	<link href="http://blog.pragmaticlab.net/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.pragmaticlab.net/blog</id>
	<author>
		<name>Le</name>
		<email>lguan@pragmaticlab.net</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://blog.pragmaticlab.net{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://blog.pragmaticlab.net{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
